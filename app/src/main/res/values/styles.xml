<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.DeviceDefault.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="background">@drawable/actionbar_layer_list</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Margins">
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginRight">50dp</item>
    </style>
    <style name="WhiteCheck">
        <item name="colorAccent">@color/white</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" ></style>


</resources>
